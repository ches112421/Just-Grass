Sample.Name = "Sample"
Sample.Parent = MainFrame
Sample.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Sample.BackgroundTransparency = 1.000
Sample.Image = "http://www.roblox.com/asset/?id=4560909609"
Sample.ImageColor3 = Color3.fromRGB(0, 0, 0)
Sample.ImageTransparency = 0.600
local function Ripple(Button)
local ms = game.Players.LocalPlayer:GetMouse()

local btn = Button
local sample = Sample

btn.MouseButton1Click:Connect(function()
	local c = sample:Clone()
	c.Parent = btn
	local x, y = (ms.X - c.AbsolutePosition.X), (ms.Y - c.AbsolutePosition.Y)
	c.Position = UDim2.new(0, x, 0, y)
	local len, size = 0.35, nil
	if btn.AbsoluteSize.X >= btn.AbsoluteSize.Y then
		size = (btn.AbsoluteSize.X * 1.5)
	else
		size = (btn.AbsoluteSize.Y * 1.5)
	end
	c:TweenSizeAndPosition(UDim2.new(0, size, 0, size), UDim2.new(0.5, (-size / 2), 0.5, (-size / 2)), 'Out', 'Quad', len, true, nil)
	for i = 1, 10 do
		c.ImageTransparency = c.ImageTransparency + 0.05
		wait(len / 12)
	end
	c:Destroy()
	end)
	end

local UIS = game:GetService("UserInputService")
function dragify(Frame)
	dragToggle = nil
	local dragSpeed = 0.50
	dragInput = nil
	dragStart = nil
	local dragPos = nil
	function updateInput(input)
		local Delta = input.Position - dragStart
		local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
		game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.30), {Position = Position}):Play()
	end
	Frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
			dragToggle = true
			dragStart = input.Position
			startPos = Frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	Frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if input == dragInput and dragToggle then
			updateInput(input)
		end
	end)
end

dragify(HolderFrame)

local Player = game.Players.LocalPlayer
local Character = Player.Character

local Mouse = Player:GetMouse()

Mouse.KeyDown:Connect(function(Key)
	if Key == "z" then 
		BoomBoxerGui:Destroy()
	end
end)

local Duping = false
local StopDuping = false
local DupeDisplayNmb

DupeBoomboxes.MouseButton1Down:Connect(function()
	Ripple(DupeBoomboxes)
	if Duping == false then
		StopDuping = false
		DupeBoomboxes.Text = "Stop Duping"
		Duping = true
		
        DupeDisplayNmb = AmountBox.Text
        
        
		local DupingAmount = AmountBox.Text
        
       



		local Blacklist = {
			"Foil",
			"BloxyCola"
		}

		local Plrs = game:GetService("Players")
		local Plr = Plrs.LocalPlayer
		local HB = game:GetService("RunService").Heartbeat

		local function GrabTools()
			for _,v in pairs(workspace:GetChildren()) do
				if v:IsA("Tool") and v.Name =="BoomBox" then
					Plr.Character.Humanoid:EquipTool(v)
				end    
			end    
		end

		local function DropTools()
			for _,v in pairs(Plr.Backpack:GetChildren()) do
				if not table.find(Blacklist, v.Name)then
					v.Parent = Plr.Character
				end    
			end
			wait()
			for _,v in pairs(Plr.Character:GetChildren()) do
				if v:IsA("Tool") then
					v.Parent = workspace
				end    
			end
		end    

		for i = 1,DupingAmount do
			if StopDuping ~= true then
			   
				Plr.Character:move(Vector3.new(2048,98765,0))
				GrabTools()
				wait(0.5)
				DropTools()
				repeat
					HB:wait()
				until not Plr.Character:FindFirstChildOfClass("Tool")
				wait()
				Plrs:Chat("-re")
				Plr.CharacterAppearanceLoaded:Wait()
				repeat HB:wait() until Plr.Character:FindFirstChild("HumanoidRootPart")
				repeat HB:wait() until Plr.Character:FindFirstChild("Humanoid")
				 DupeDisplayNmb = DupeDisplayNmb - 1
			     AmountBox.Text = DupeDisplayNmb
			else
				break
			end
		end

		Plr.Character:move(Vector3.new(0,10,0))
		GrabTools()
		Duping = false
		DupeBoomboxes.Text = "Dupe"
	end
	if Duping == true then
		StopDuping = true
		Duping = false
		DupeBoomboxes.Text = "Dupe"
		GrabTools()
	end
end)

DropBoomboxes.MouseButton1Down:Connect(function()
	Ripple(DropBoomboxes)
	for _,v in pairs(Player.Backpack:GetChildren()) do
		if v.Name == "BoomBox" then
			v.Parent = Player.Character
		end
		wait()
		for _,v in pairs(Player.Character:GetChildren()) do
			if v.Name == "BoomBox" then
				v.Parent = workspace
			end
		end	
	end
end)

local StealingBoomBoxes = false

StealBoomboxes.MouseButton1Down:Connect(function()
	Ripple(StealBoomboxes)
	if StealingBoomBoxes == false then 
	    StealingBoomBoxes = true
	    
	    StealBoomboxes.Text = "Stop Stealing"
	    
	    while StealingBoomBoxes == true do
	        task.wait()
	    for _,v in pairs(workspace:GetChildren()) do
	        task.wait()
		    if v:IsA("Tool") and v.Name == "BoomBox" then
		    	Player.Character.Humanoid:EquipTool(v)
		    end
	    end
	    end
	    
end
	if StealingBoomBoxes == true then
	    StealBoomboxes.Text = "Steal Boomboxes"
	    StealingBoomBoxes = false
	   end 
end)

MassPlay.MouseButton1Down:Connect(function()
	Ripple(MassPlay)
	for _,v in pairs(Player.Backpack:GetChildren()) do
		if v.Name == "BoomBox" then
			v.Parent = Player.Character
		end
	end
	wait(0.4)
	for _,v in pairs(Player.Character:GetChildren()) do
		if v.Name == "BoomBox" then
			v.RemoteEvent:FireServer(AudioIDBox.Text)
		end
	end
end)

CountBoomboxes.MouseButton1Down:Connect(function()
    local amt = 0
    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
    for _,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do 
        if v.Name == "BoomBox" then
            amt = amt + 1
        end
    end   
    --game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack({[1] = tostring(amt).." Boomboxes",[2] = "All"}))
    game.StarterGui:SetCore("SendNotification",{Title = "Boombox Count", Text = "You have "..tostring(amt).." Boomboxes"})
end)    
